{
  "name": "@akidolabs/audio-waveform-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./build/server.js\"",
    "dev:build": "NODE_ENV=development webpack --watch --display-error-details --color --progress",
    "dev:start": "just-wait -t 180 -p \"build/server.js\" && nodemon build/server.js",
    "build": "NODE_ENV=production webpack --display-error-details",
    "start": "NODE_ENV=production node build/server",
    "lint": "eslint --ext=ts,js --report-unused-disable-directives .",
    "init": "eslint --init"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "couchbase": "^2.6.12",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-promise-router": "^3.0.3",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/couchbase": "^2.4.2",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.6",
    "@types/express-promise-router": "^3.0.0",
    "@types/jest": "^26.0.3",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.7",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.6",
    "uuid": "^8.2.0"
  }
